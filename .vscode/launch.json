    "version": "0.2.0",
    "configurations": [
        {
            "name": "p2p_node1",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/example/pointserver/p2pserver",
            "args": ["127.0.0.1","12101","12102"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/example",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "p2p_node2",
